{
  "name": "datacenter",
  "version": "1.0.0",
  "description": "ktt data collection",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "cli": "./bin/cli.js",
    "bill": "./bin/bill.bat"
  },
  "scripts": {
    "build": "npm run lint && npm run prettier && tsc && cp src/*.json dist/",
    "clean": "rm -fr dist/*",
    "cli-paidan": "ts-node src/cli.ts paidan ../test ../test/bak",
    "cli-huidan": "ts-node src/cli.ts huidan E:/ktt/express/0812 ../test/bak",
    "cli-client": "ts-node src/cli.ts c ../test ../test/bak",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint --fix src --ext .ts --max-warnings=10",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node src/cli.ts paidan ../test ../test/bak",
    "filter": "ts-node src/cli.ts filter 045 E:/ktt/express/2023-05/0521 ../test/bak"
  },
  "author": "bill",
  "license": "ISC",
  "dependencies": {
    "commander": "^10.0.0",
    "diff": "^5.1.0",
    "exceljs": "^4.3.0",
    "fast-formula-parser": "^1.0.19",
    "formulajs": "^1.0.8",
    "hot-formula-parser": "^4.0.0",
    "jStat": "^1.8.6",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3",
    "uglify-js": "^3.17.4",
    "vows": "^0.8.3"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
